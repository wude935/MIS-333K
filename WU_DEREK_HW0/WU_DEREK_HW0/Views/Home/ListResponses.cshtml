@model IEnumerable<GuestResponse>
<h2>Here is the list of people attending the party</h2>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>
    <tbody>
        @foreach (GuestResponse gr in Model)
        {
            <tr>
                <td>@gr.Name</td>
                <td>@gr.Email</td>
                <td>@gr.Phone</td>
            </tr>
        }
    </tbody>
</table>


